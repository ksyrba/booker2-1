<% unless @user.id == current_user.id %>
   <% if @haveRoom == true %>
     <%= link_to "chatを始める", user_rooms_path(@room.id) %>
   <% else %>
     <%= form_with model: [@user, @room], local: true do |f| %>
       <%= fields_for @roomUser do |r| %>
         <%= r.hidden_field :user_id, value: @user.id %>
       <% end %>
       <%= f.submit "chatを始める" %>
     <% end %>
   <% end %>
<% end %>